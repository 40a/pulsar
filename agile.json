{
    "python": {
        "pulsar_version": "pulsar.__version__"
    },
    "github": {
        "pulsar": {
            "version": "{{ pulsar_version }}",
            "release_notes": "docs/source/history"
        }
    },
    "shell": {
        "pulsar": {
            "command": [
                "rm -rf dist",
                "{{ python }} setup.py sdist bdist_wheel upload"
            ]
        }
    }
}
