[run]
source = pulsar,examples
omit =
    *pulsar/utils/_py2.py
    *pulsar/utils/fallbacks/*
    *pulsar/utils/system/winservice.py
    *pulsar/utils/system/windowssystem.py
    *pulsar/utils/system/winprocess.py
    *pulsar/utils/cov.py
    *pulsar/async/cov.py
    examples/httpbin/config.py
    examples/philosophers/config.py
    pshell,examples/taskqueue/winservice.py
    examples/pshell/*
    examples/philosophers/*
    *pulsar/apps/test/plugins/bench.py
    *pulsar/apps/test/plugins/profile.py
    *pulsar/apps/tasks/backends/redis.py
    examples/taskqueue/test_redis.py
    *pulsar/async/udp.py
    *pulsar/apps/green/*


[report]
# Regexes for lines to exclude from consideration
exclude_lines =
    (?i)# *pragma[: ]*no *cover
    raise NotImplementedError
